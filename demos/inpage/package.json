{
  "name": "@getwax/inpage",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "setup": "yarn && scripts/setup.ts",
    "setup:testnet": "yarn && scripts/setup-testnet.ts",
    "dev": "scripts/dev.ts",
    "check-ts": "tsc --noEmit",
    "build": "yarn check-ts && rm -rf build && yarn build:lib && yarn build:demo && yarn build:globalScript && yarn build:packed-lib",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:packed-lib": "npm pack --pack-destination build/.",
    "build:demo": "vite build",
    "build:globalScript": "BUILD_GLOBAL_SCRIPT=true vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@s-libs/micro-dash": "^16.0.0",
    "@types/color": "^3.0.3",
    "color": "^4.2.3",
    "ethers": "^6.9.0",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.75",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "events": "^3.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-plugin-node-polyfills": "^0.17.0"
  }
}
